<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="usersearch">
	
	
	<!-- 아이디검색 -->
	<select id="select_id_bio" parameterType="string" resultType="com.sns.user.db.UserVO">
	
	SELECT u_id
	FROM USER_INFO
	WHERE U_ID LIKE '%'||#{u_id}||'%'
	
	
	</select>


</mapper>